<Window x:Class="RPG_WPF.death"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RPG_WPF"
        mc:Ignorable="d"
        Title="death" Height="450" Width="800" Background="#B0D7FF">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90*"/>
            <RowDefinition Height="230*"/>
            <RowDefinition Height="70*"/>
        </Grid.RowDefinitions>
        <Label x:Name="DeathLabel" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20"></Label>
    </Grid>
</Window>
